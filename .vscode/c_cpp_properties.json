{
    "configurations": [
        {
            "name": "ESP32",
            "compilerPath": "${env:HOME}/Library/Arduino15/packages/esp32/tools/xtensa-esp32s3-elf-gcc/esp-2021r2-patch5-8.4.0/bin/xtensa-esp32s3-elf-gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm",
            "includePath": [
                "${workspaceFolder}/**",
                "${env:HOME}/Library/Arduino15/packages/esp32/tools/xtensa-esp32s3-elf-gcc/*/xtensa-esp32s3-elf/include/**",
                "${env:HOME}/Library/Arduino15/packages/esp32/hardware/esp32/2.0.7/**",
                "${env:HOME}/Library/Arduino15/packages/esp32/hardware/esp32/2.0.7/libraries/**/src",
                "${env:HOME}/Documents/Arduino/libraries/**/src"
            ],
            "defines": [
                "ARDUINO=10819",
                "ARDUINO_ESP32S3_DEV",
                "ARDUINO_ARCH_ESP32",
                "ESP32",
                "F_CPU=240000000L",
                "CORE_DEBUG_LEVEL=0",
                "BOARD_HAS_PSRAM"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/**",
                    "${env:HOME}/Library/Arduino15/packages/esp32/tools/xtensa-esp32s3-elf-gcc/*/xtensa-esp32s3-elf/include/**",
                    "${env:HOME}/Library/Arduino15/packages/esp32/hardware/esp32/2.0.7/**"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}
